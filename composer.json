{
    "name": "neurony/laravel-revisions",
    "description": "Create multiple revisions of any Eloquent model record along with its underlying relationships",
    "license": "MIT",
    "keywords": [
        "laravel",
        "revisions",
        "eloquent",
        "model",
        "relations",
        "changes",
        "copy"
    ],
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Andrei Badea",
            "email": "zbiller@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/contracts": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "~7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Neurony\\Revisions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neurony\\Revisions\\Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neurony\\Revisions\\ServiceProvider"
            ]
        }
    }
}
